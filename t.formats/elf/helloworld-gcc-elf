0x08048309                54  push esp
'
run_test

NAME='pi 12'
FILE=../../bins/elf/x86-helloworld-gcc
CMDS='pi 12
'
EXPECT='xor ebp, ebp
pop esi
mov ecx, esp
and esp, 0xfffffff0
push eax
push esp
push edx
push 0x8048490
push 0x8048420
push ecx
push esi
push 0x8048400
'
run_test


