NAME='pe string'
IGNORE_ERR=1
BROKEN=
FILE=$F
ARGS=
CMDS='
e asm.lines=false
e asm.bytes=false
e asm.comments=false
pd 1@0x0040131a~str.Hello
'
EXPECT=' 0x0040131a     mov dword [esp], str.Hello_n
'
run_test


NAME='pe32 imported libs (il)'
BROKEN=
FILE=../../bins/pe/imports_1210.exe
