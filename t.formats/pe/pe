#!/bin/sh

# GPL 3+ - Copyright (C) 2013-2015  pancake

for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done
F=../../bins/pe/base.exe

NAME='pe entrypoint'
IGNORE_ERR=1
FILE=$F
ARGS=
CMDS='ie~addr
'
EXPECT='vaddr=0x00401280 paddr=0x00000680 baddr=0x00400000 laddr=0x00000000
'
run_test

NAME='pe relocs [r2]'
IGNORE_ERR=1
FILE=$F
ARGS=
CMDS='e asm.functions = false
e asm.flags = false
pd 1@0x401820~!imp
'
EXPECT=''
run_test

NAME='pe relocs [rabin2 a.exe]'
FILE=../../bins/pe/a.exe
ARGS=
CMDS="!rabin2 -R ${FILE}"
EXPECT='[Relocations]
