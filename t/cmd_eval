#!/bin/sh

[ -e tests.sh ] && . ./tests.sh || . ../tests.sh


NAME='e asm.os'
FILE=../bins/elf/hello-linux-x86_64
CMDS='e asm.os=?
'
EXPECT='dos
darwin
linux
freebsd
openbsd
netbsd
windows
'

run_test

NAME='e asm.cpu'
FILE=../bins/elf/hello-linux-x86_64
CMDS='e asm.arch=mips
e asm.cpu=?
'
EXPECT='gp64
micro
r6
v3
'

run_test

NAME='e asm.syntax'
FILE=-
CMDS='wx eb0e
e asm.syntax=masm
pi 1
'
EXPECT='jmp 10h
'

run_test
