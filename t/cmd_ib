for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='ib java class file'
BROKEN=
FILE=malloc://1024
ARGS=
CMDS='
e asm.comments=false
e asm.cmtflgrefs=false
e scr.color=false
e asm.lines=false
e asm.xrefs=false
wx 
ib
if
af
pdf
'
EXPECT='type     JAVA CLASS
